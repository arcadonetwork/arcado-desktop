    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js"
  },
  "lint-staged": {
    "**/*.{js,jsx,less}": [
      "prettier --write",
      "git add"
    ],
    "**/*.{js,jsx}": "npm run lint-staged:js"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "eslintConfig": {
    "extends": "react-app"
  }
