    "react-router-dom": "5.2.0"
  },
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "precommit": "lint-staged",
    "test": "roadhog test",
    "eject": "craco eject",
    "lint": "eslint --ext .js src mock tests && npm run lint:style",
