    "less": "3.11.1"
  },
  "dependencies": {
    "@craco/craco": "^5.6.4",
    "@rematch/core": "1.4.0",
    "@types/jest": "25.2.2",
    "@types/node": "14.0.1",
    "@types/react": "16.9.35",
    "@types/react-dom": "16.9.8",
    "@types/react-redux": "7.1.0",
    "@types/react-router-dom": "5.1.5",
